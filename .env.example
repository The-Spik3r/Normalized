# Ejemplo de archivo de configuración de entorno
# Copia este archivo como .env y modifica los valores según necesites

# Configuración de base de datos (opcional)
# DB_HOST=localhost
# DB_PORT=3306
# DB_NAME=mi_base_de_datos
# DB_USER=mi_usuario
# DB_PASSWORD=mi_password

# Configuración de logging
# LOG_LEVEL=INFO
# LOG_FILE=csv_to_sql.log

# Configuración por defecto del convertidor
# DEFAULT_CHUNK_SIZE=1000
# DEFAULT_MAX_ROWS=None
# DEFAULT_TABLE_PREFIX=converted_

# Rutas por defecto
# DEFAULT_OUTPUT_DIR=./output
# DEFAULT_CSV_DIR=./data